class Time:
  def printTime(time):
    print str(time.hours) + ":" +  \
          str(time.minutes) + ":" +  \
          str(time.seconds)
  def __init__(time,hours=0,minutes=0,seconds=0):
     time.hours=hours
     time.minutes=minutes
     time.seconds=seconds 

#currentTime=Time()
#currentTime.hours=9
#currentTime.minutes=14
#currentTime.seconds=30
cureentTime=Time(10,20,30)
currentTime.printTime()
